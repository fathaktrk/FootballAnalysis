@model Compare
<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
        google.charts.load('current', { 'packages': ['corechart'] });
        google.charts.setOnLoadCallback(drawChart);

        function drawChart() {
            
            var TeamOneName = "@Model.TeamOneName";
            var TeamTwoName = "@Model.TeamTwoName";
            var TeamOneWinAll = @Model.TeamOneWinAll;
            var TeamTwoWinAll = @Model.TeamTwoWinAll;
            var DrawCount  = @Model.DrawCount;

            var data = google.visualization.arrayToDataTable([
                ['Task', 'Hours per Day'],

                [TeamOneName, TeamOneWinAll],
                [TeamTwoName, TeamTwoWinAll],
                ['Beraberlik', DrawCount]

            ]);

            var options = {
                title: 'Tüm Maçlarda Kazanma ve Beraberlik Oranları'
            };

            var chart = new google.visualization.PieChart(document.getElementById('piechart'));

            chart.draw(data, options);
        }
    </script>
</head>
<body>
    <div id="piechart"></div>
</body>
